# Erzeuge ein quadratisches Fenster
size(500, 500)
# Farbe OrangeRed auf SkyBlue
background("#87CEEB")
fill("#FF4500")       

# Zeichne Kreis mit Durchmesser 42
circle(40, 25, 42)    # Wo ist das?
circle(400, 25, 42)   # ...und jetzt?
circle(40, 250, 42)   # ...und jetzt?
circle(40, 2500, 42)  # ...und jetzt?
circle(width-40, 25, 42) # ... jetzt?
circle(40, height-25, 42) # ...jetzt?
circle(0.5*width, 0.3*height, 42) # ?
